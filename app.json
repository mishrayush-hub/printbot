{
  "expo": {
    "name": "Printbot",
    "slug": "hello-to-instant-printing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android/icon-light.png",
    "scheme": "printbot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "cloud.printbot.app",
      "icon": "./assets/images/ios/printbot.icon",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow PrintBot to access your photos to upload and print documents. This permission is required to select files from your photo library to upload to our servers.",
        "NSPhotoLibraryAddUsageDescription": "Allow PrintBot to save printed documents to your photo library. This permission is required to select printed documents after processing in our servers."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/adaptive-icon.png",
        "monochromeImage": "./assets/images/android/adaptive-icon.png",
        "backgroundColor": "#008cff"
      },
      "icon":  "./assets/images/android/icon-light.png",
      "package": "cloud.printbot.app",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/web/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-bottom-tabs",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash/splash-black.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#008cff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "extraPods": [
              { "name": "SDWebImage", "modular_headers": true },
              { "name": "SDWebImageSVGCoder", "modular_headers": true },
              { "name": "SDWebImageWebPCoder", "modular_headers": true }
            ]
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "34266abe-4562-4e3b-9c7c-2ee4c705d888"
      }
    }
  }
}
