{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "store"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "ios" : {
        "image" : "latest",
        "buildConfiguration": "Release",
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
